{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["dom", "es6"],
    "module": "commonjs",
    "target": "ES3",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
